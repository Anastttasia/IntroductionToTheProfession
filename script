//Задание 7
if (git status -s == 'dev')
then
    git diff > mychange.diff
    git checkout prd
    git apply mychange.diff

    git commit
    echo 'Введи тэг'
    tttag <- read (считай имя тэга с клавы и засунь в переменную tttag)
    git tag tttag
    git push
else
    echo 'Ты уже на ветке prd коммить и пуш'

